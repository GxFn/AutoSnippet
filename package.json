{
  "name": "autosnippet",
  "version": "1.7.0",
  "description": "AutoSnippet Dashboard - 连接开发者、AI 与项目知识库的工具",
  "main": "index.js",
  "scripts": {
  "postinstall": "node scripts/ensure-parse-package.js && node scripts/build-native-ui.js && node scripts/build-asd-entry.js",
  "generate-checksums": "node scripts/generate-checksums.js",
  "verify-checksums": "node scripts/verify-checksums.js",
  "prepublishOnly": "npm run generate-checksums",
  "build:parser": "cd tools/parse-package && swift build -c release",
  "build:native-ui": "swiftc resources/native-ui/main.swift resources/native-ui/combined-window.swift -o resources/native-ui/native-ui -framework AppKit",
  "install:cursor-skill": "node scripts/install-cursor-skill.js",
  "install:cursor-skill:mcp": "node scripts/install-cursor-skill.js --mcp",
  "install:vscode-copilot": "node scripts/install-vscode-copilot.js",
  "install:full": "node scripts/install-full.js",
  "verify:context-api": "node scripts/verify-context-api.js",
  "setup:mcp": "node scripts/setup-mcp-config.js --editor vscode && node scripts/setup-mcp-config.js --editor cursor",
  "build:dashboard": "cd dashboard && npm run build",
  "dashboard": "node bin/asd-cli.js ui",
  "test": "node test/runner.js",
  "test:unit": "node test/unit/run-all.js",
  "test:changed": "node test/runner.js --changed",
  "test:global": "node test/runner.js --global",
  "test:integration": "node test/integration/run-tests.js",
  "test:integration:recipes": "node test/integration/suites/recipes.test.js",
  "test:integration:permissions": "node test/integration/suites/permissions.test.js",
  "test:integration:cross-project": "node test/integration/suites/cross-project.test.js",
  "dev:link": "npm install -g .",
  "dev:verify": "which asd && asd -v",
  "release:check": "node scripts/release.js check",
  "release:patch": "node scripts/release.js patch",
  "release:minor": "node scripts/release.js minor",
  "release:major": "node scripts/release.js major",
  "dev:ui": "ASD_UI_BUILD=1 node bin/asd-cli.js ui",
  "test:init": "node bin/asd-cli.js init",
  "test:version": "node bin/asd-cli.js -v",
  "test:help": "node bin/asd-cli.js -h",
  "test:install": "node bin/asd-cli.js i",
  "test:create": "node bin/asd-cli.js c",
  "test:share": "node bin/asd-cli.js s",
  "test:update": "node bin/asd-cli.js u",
  "test:watch": "node bin/asd-cli.js w",
  "test:root": "node bin/asd-cli.js root",
  "init:snippets": "node scripts/init-xcode-snippets.js init",
  "list:snippets": "node scripts/init-xcode-snippets.js list",
  "remove:snippets": "node scripts/init-xcode-snippets.js remove",
  "diagnose:mcp": "node scripts/diagnose-mcp.js"
  },
  "author": "gaoxuefeng",
  "license": "MIT",
  "bin": {
  "asd": "bin/asd"
  },
  "keywords": [
  "Xcode",
  "iOS",
  "Snippet"
  ],
  "homepage": "https://github.com/GxFn/AutoSnippet#readme",
  "repository": {
  "type": "git",
  "url": "git+https://github.com/GxFn/AutoSnippet.git"
  },
  "dependencies": {
  "@google/generative-ai": "^0.21.0",
  "@modelcontextprotocol/sdk": "^1.25.3",
  "@zilliz/milvus2-sdk-node": "^2.5.10",
  "axios": "^1.6.7",
  "chokidar": "^3.6.0",
  "commander": "^7.2.0",
  "cors": "^2.8.5",
  "dotenv": "^16.3.1",
  "express": "^4.18.2",
  "inquirer": "^8.0.0",
  "minimist": "^1.2.5",
  "open": "^8.0.4",
  "undici": "^6.23.0",
  "xml2js": "^0.6.2",
  "zod": "^4.3.6"
  },
  "bugs": {
  "url": "https://github.com/GxFn/AutoSnippet/issues"
  },
  "files": [
  "bin",
  "lib",
  "scripts",
  "skills",
  "recipes",
  "dashboard/dist",
  "index.js",
  "template.json",
  "tools/parse-package/Package.swift",
  "tools/parse-package/Package.resolved",
  "tools/parse-package/README.md",
  "tools/parse-package/Sources",
  "resources/native-ui/main.swift",
  "resources/native-ui/README.md",
  "resources/asd-entry/main.swift",
  "checksums.json",
  "resources/openChrome.applescript"
  ],
  "directories": {
  "doc": "docs",
  "lib": "lib",
  "test": "test"
  },
  "devDependencies": {
  "mocha": "^11.3.0"
  }
}
