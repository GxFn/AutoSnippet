{
	"name": "autosnippet",
	"version": "1.4.3",
	"description": "A iOS module management tool.",
	"main": "index.js",
	"scripts": {
		"postinstall": "node scripts/ensure-parse-package.js && node scripts/build-native-ui.js",
		"build:parser": "cd tools/parse-package && swift build -c release",
		"build:native-ui": "swiftc tools/native-ui/main.swift -o bin/native-ui -framework AppKit",
		"install:cursor-skill": "node scripts/install-cursor-skill.js",
		"install:cursor-skill:mcp": "node scripts/install-cursor-skill.js --mcp",
		"install:full": "node scripts/install-full.js",
		"verify:context-api": "node scripts/verify-context-api.js",
		"build:dashboard": "cd dashboard && npm run build",
		"test": "node test/runner.js",
		"test:unit": "node test/unit/run-all.js",
		"test:changed": "node test/runner.js --changed",
		"test:global": "node test/runner.js --global",
		"dev:link": "npm install -g .",
		"dev:verify": "which asd && asd -v",
		"dev:ui": "ASD_UI_BUILD=1 node bin/asnip.js ui",
		"test:init": "node bin/asnip.js init",
		"test:version": "node bin/asnip.js -v",
		"test:help": "node bin/asnip.js -h",
		"test:install": "node bin/asnip.js i",
		"test:create": "node bin/asnip.js c",
		"test:share": "node bin/asnip.js s",
		"test:update": "node bin/asnip.js u",
		"test:watch": "node bin/asnip.js w",
		"test:root": "node bin/asnip.js root"
	},
	"author": "gaoxuefeng",
	"license": "MIT",
	"bin": {
		"asd": "bin/asnip.js"
	},
	"keywords": [
		"Xcode",
		"iOS",
		"Snippet"
	],
	"homepage": "https://github.com/GxFn/AutoSnippet#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GxFn/AutoSnippet.git"
	},
	"dependencies": {
		"@google/generative-ai": "^0.21.0",
		"@modelcontextprotocol/sdk": "^1.25.3",
		"axios": "^1.6.7",
		"chokidar": "^3.6.0",
		"commander": "^7.2.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"inquirer": "^8.0.0",
		"minimist": "^1.2.5",
		"open": "^8.0.4",
		"undici": "^6.23.0",
		"xml2js": "^0.6.2",
		"zod": "^4.3.6"
	},
	"bugs": {
		"url": "https://github.com/GxFn/AutoSnippet/issues"
	},
	"files": [
		"bin",
		"lib",
		"scripts",
		"skills",
		"recipes",
		"dashboard/dist",
		"index.js",
		"template.json",
		"tools/parse-package/Package.swift",
		"tools/parse-package/Package.resolved",
		"tools/parse-package/README.md",
		"tools/parse-package/Sources",
		"tools/native-ui/main.swift"
	],
	"directories": {
		"doc": "docs",
		"lib": "lib",
		"test": "test"
	},
	"optionalDependencies": {
		"@lancedb/lancedb": "^0.23.0"
	},
	"devDependencies": {}
}
