{
    "name": "autosnippet",
    "version": "1.2.17",
    "description": "A iOS module management tool.",
    "main": "index.js",
    "scripts": {
        "test": "node bin/asnip.js init",
        "test:init": "node bin/asnip.js init",
        "test:version": "node bin/asnip.js -v",
        "test:help": "node bin/asnip.js -h",
        "test:install": "node bin/asnip.js i",
        "test:create": "node bin/asnip.js c",
        "test:share": "node bin/asnip.js s",
        "test:update": "node bin/asnip.js u",
        "test:watch": "node bin/asnip.js w",
        "test:root": "node bin/asnip.js root",
        "test:global:install": "npm install -g .",
        "test:global:install:online": "npm install -g autosnippet",
        "test:global:install:version": "npm install -g autosnippet@",
        "test:global:uninstall": "npm uninstall -g autosnippet",
        "test:global:verify": "which asd && asd -v",
        "test:global:info": "npm list -g autosnippet",
        "test:cross:dir": "node scripts/test-cross-directory.js"
    },
    "author": "gaoxuefeng",
    "license": "MIT",
    "bin": {
        "asd": "bin/asnip.js"
    },
    "keywords": [
        "Xcode",
        "iOS",
        "Snippet"
    ],
    "homepage": "https://github.com/GxFn/AutoSnippet#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GxFn/AutoSnippet.git"
    },
    "dependencies": {
        "chokidar": "^3.6.0",
        "commander": "^7.2.0",
        "inquirer": "^8.0.0",
        "minimist": "^1.2.5",
        "open": "^8.0.4",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {},
    "bugs": {
        "url": "https://github.com/GxFn/AutoSnippet/issues"
    },
    "directories": {
        "doc": "docs",
        "lib": "lib"
    }
}
